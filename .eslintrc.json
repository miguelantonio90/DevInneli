{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": 0,
        "no-mixed-spaces-and-tabs": 0
    }
}
